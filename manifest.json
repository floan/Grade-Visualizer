{
  "manifest_version": 2,
  "name": "Rate My Professor Grade Visualizer",
  "version": "1.0",
  "content_scripts" : [
  	{
  		"matches" : [
  			"*://*.ratemyprofessors.com/*" //This indicates that the extension only works with ratemyprof pages
  		],
  		"js": ["content.js"]
  	}
  ],
  "background": {
  	"scripts": ["background.js"]
  },
  "browser_action" : {
  	"default_icon": "xyz.png"
  }
  
}